{
  "name": "searchsocket",
  "version": "0.1.0",
  "description": "Semantic site search and MCP retrieval for SvelteKit static sites",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "searchsocket": "dist/cli.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./sveltekit": {
      "types": "./dist/sveltekit.d.ts",
      "import": "./dist/sveltekit.js",
      "require": "./dist/sveltekit.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.29.2",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@pinecone-database/pinecone": "^7.0.0",
    "@zilliz/milvus2-sdk-node": "^2.6.9",
    "better-sqlite3": "^12.6.2",
    "cheerio": "^1.2.0",
    "chokidar": "^5.0.0",
    "commander": "^14.0.3",
    "express": "^5.2.1",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "jiti": "^2.6.1",
    "openai": "^6.19.0",
    "p-limit": "^7.3.0",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "@types/turndown": "^5.0.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
